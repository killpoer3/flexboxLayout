<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="flex.com">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AlignContentActivity" />
        <activity android:name=".AlignItemsActivity" />
        <activity android:name=".DividerActivity" />
        <activity android:name=".FlexDirectionActivity" />
        <activity android:name=".FlexRecyclerActivity" />
        <activity android:name=".FlexWrapActivity" />
        <activity android:name=".JustifyContentActivity" />
        <activity android:name=".LayoutAlignSelfActivity" />
        <activity android:name=".LayoutFlexBasisPercentActivity" />
        <activity android:name=".LayoutFlexGrowActivity" />
        <activity android:name=".LayoutFlexShrinkActivity" />
        <activity android:name=".LayoutOrderActivity" />
    </application>

</manifest>